{
    "name": "jeffrey-kroonen/bol-retailer-api-php",
    "description": "A PHP Client to interact with the Bol.com Retailer API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "role": "Developer",
            "name": "Jeffrey Kroonen"
        }
    ],
    "autoload": {
        "psr-4": {
            "JeffreyKroonen\\BolRetailer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JeffreyKroonen\\BolRetailer\\Tests\\": "tests/"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "^7.4",
        "jane-php/json-schema-runtime": "^7.4",
        "symfony/http-foundation": "^6.1",
        "phpseclib/phpseclib": "^3.0"
    },
    "require-dev": {
        "vimeo/psalm": "^4.26",
        "phpstan/phpstan": "^1.8",
        "friendsofphp/php-cs-fixer": "^3.10",
        "jane-php/json-schema": "^7.4",
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "conifg": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
