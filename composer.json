{
    "name": "jeffrey-kroonen/bol-retailer-api-php",
    "description": "A PHP Client to interact with the Bol.com Retailer API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "role": "Developer",
            "name": "Jeffrey Kroonen"
        }
    ],
    "autoload": {
        "psr-4": {
            "JeffreyKroonen\\BolRetailer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JeffreyKroonen\\BolRetailer\\Tests\\": "tests/"
        }
    },
    "require": {
        "guzzlehttp/guzzle": "7.4.x-dev",
        "jane-php/json-schema-runtime": "7.x-dev",
        "symfony/http-foundation": "6.2.x-dev"
    },
    "require-dev": {
        "vimeo/psalm": "5.x-dev",
        "phpstan/phpstan": "1.8.x-dev",
        "friendsofphp/php-cs-fixer": "dev-master",
        "jane-php/json-schema": "7.x-dev",
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "conifg": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
